{
  "version": 2,
  "builds": [
    {
      "src": "./front-end/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "back-end/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/back-end/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/front-end/build/$1"
    }
  ]
}
